<div class="container">
  <div class="page-header">

    <h1>New Project</h1>
    <hr>

    <%= form_for @project, html: { class: "form-horizontal" } do |f| %>

    <div class="form-group">
      <%= f.label :project_name, "Name", class: "col-md-2 control-label" %>
      <div class="col-md-6">
        <%= f.text_field :project_name, class: "form-control" %>
      </div>
    </div>

    <div class="form-group">
      <div class="col-md-offset-2 col-md-3">
        <%= f.submit "Create Project", class: "btn btn-primary" %>
        <%= link_to "Cancel", projects_path, class: "btn btn-default" %>
      </div>
    </div>

<% end %>



  </div>
</div>
